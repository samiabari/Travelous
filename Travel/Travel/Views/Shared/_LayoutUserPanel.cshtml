        <li>
            <a href="/Home/LogOut"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
        </li>
    </ul>

}
 </li>
      @if(((string)Session["userCode"]=="vendor")||((string)Session["userCode"] == "tourist"))
