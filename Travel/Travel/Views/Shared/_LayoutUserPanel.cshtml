                                int touristId = (int)Session["TouristId"];
                                if (Session["pp"] != null)
                                {
                                    byte[] image = (byte[])Session["pp"];
                                    <li class="active">
                                        <a href="#"> <img src="data:image;base64,@System.Convert.ToBase64String(image)" alt="@Session["TouristName"]" style="width:215px;margin:3px;height:250px;"></a>
                                    </li>
                                }
                                else
                                {

